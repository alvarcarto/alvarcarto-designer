{
  "name": "alvarcarto-designer",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "last 3 versions",
    "ie 9"
  ],
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-plugin-import": "^1.1.1",
    "chokidar-cli": "^1.2.0",
    "clean-css-cli": "^4.0.6",
    "concurrently": "^3.3.0",
    "gh-pages": "^0.12.0",
    "less": "^2.7.2",
    "less-plugin-glob": "^2.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "postcss-cli": "^2.6.0",
    "react-scripts": "git+https://github.com/kimmobrunfeldt/react-scripts-antd.git#master",
    "redux-logger": "^2.7.4"
  },
  "dependencies": {
    "@google/maps": "^0.3.1",
    "alvarcarto-common": "git+https://alvarcarto-integration:1c9888484a1db3a46fb46c4d465ea4edc6d93196@github.com/kimmobrunfeldt/alvarcarto-common.git#v3.1.1",
    "alvarcarto-price-util": "git+https://alvarcarto-integration:1c9888484a1db3a46fb46c4d465ea4edc6d93196@github.com/kimmobrunfeldt/alvarcarto-price-util.git#v2.0.2",
    "antd": "^2.6.3",
    "auto-prefixer": "^0.4.2",
    "axios": "^0.15.3",
    "bluebird": "^3.4.7",
    "common-tags": "^1.4.0",
    "debounce-promise": "^2.1.1",
    "geodist": "^0.2.1",
    "history": "^4.5.1",
    "i18n-iso-countries": "^1.8.0",
    "leaflet": "github:kimmobrunfeldt/Leaflet#master",
    "normalize.css": "^5.0.0",
    "odometer": "^0.4.7",
    "raven-js": "^3.12.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-imageloader": "^2.1.0",
    "react-leaflet": "^1.1.2",
    "react-redux": "^4.4.6",
    "react-responsive": "^1.2.7",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "request-promise": "^4.1.1",
    "webfontloader": "^1.6.27"
  },
  "scripts": {
    "minify-css": "cleancss ./src/bundle.css -o ./src/bundle.css",
    "build-css": "lessc --npm-import --glob ./src/styles/index.less ./src/bundle.css && npm run postcss",
    "postcss": "postcss --use autoprefixer ./src/bundle.css -o ./src/bundle.css",
    "watch-css": "npm run build-css && chokidar \"src/**/*.less\" \"./src/styles/**/*.css\" -c \"npm run build-css\"",
    "start": "concurrently \"react-scripts start\" \"npm run watch-css\"",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && gh-pages -d build"
  }
}
